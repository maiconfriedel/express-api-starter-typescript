{
  "name": "express-api-starter-typescript",
  "version": "1.2.1",
  "description": "A basic starter for an express.js API using Typescript. Forked from w3cj/express-api-starter",
  "main": "index.ts",
  "scripts": {
    "start": "tsc && node dist/src/index.js",
    "build": "tsc",
    "dev": "tsnd --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "lint": "eslint src/**/*.ts --fix",
    "docker": "docker-compose up -d"
  },
  "keywords": [],
  "author": "Maicon Gabriel Friedel <maicon.friedel@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/maiconfriedel/express-api-starter-typescript.git"
  },
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^9.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
